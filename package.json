{
	"name": "agds-next",
	"version": "0.0.1",
	"private": true,
	"license": "MIT",
	"main": "index.js",
	"workspaces": [
		"packages/*",
		"docs",
		"example-site",
		"example-form"
	],
	"scripts": {
		"build": "preconstruct build",
		"clean": "rimraf \"**/node_modules\" \"**/yarn-error.log\" \"**/out.log\" \"**/.DS_Store\" \"**/dist\" \"**/out\" \"**/.next\"",
		"docs:build": "yarn --cwd docs build",
		"docs:dev": "yarn --cwd docs dev",
		"docs:generate-component-props": "yarn --cwd docs generate-component-props",
		"docs:generate-template-images": "yarn --cwd docs generate-template-images",
		"example-form:build": "yarn --cwd example-form build",
		"example-form:dev": "yarn --cwd example-form dev",
		"example:build": "yarn --cwd example-site build",
		"example:dev": "yarn --cwd example-site dev",
		"format": "prettier --write \"**/*.{js,jsx,json,ts,tsx,md,mdx,yml,yaml}\"",
		"fresh": "yarn clean && yarn",
		"postinstall": "manypkg check && preconstruct dev",
		"lint": "yarn lint:packages && yarn lint:storybook && yarn lint:example && yarn lint:example-form && yarn lint:docs && yarn lint:prettier && yarn lint:typescript",
		"lint:docs": "yarn --cwd docs lint",
		"lint:example": "yarn --cwd example-site lint",
		"lint:example-form": "yarn --cwd example-form lint",
		"lint:packages": "yarn eslint \"packages/**/*.{js,jsx,ts,tsx}\"",
		"lint:prettier": "prettier --check \"**/*.{js,jsx,json,ts,tsx,md,mdx,yml,yaml}\"",
		"lint:storybook": "yarn eslint \".storybook/**/*.{js,jsx,ts,tsx}\"",
		"lint:typescript": "tsc --noEmit",
		"new:package": "plop --plopfile scripts/plopfile.mjs && yarn format && yarn manypkg fix && yarn install",
		"playroom:build": "yarn --cwd docs playroom:build",
		"playroom:dev": "yarn --cwd docs playroom:dev",
		"publish-changed": "yarn build && yarn changeset publish",
		"storybook:build": "storybook build -o docs/public/storybook",
		"storybook:dev": "storybook dev --port 6006 --no-open",
		"test": "jest"
	},
	"devDependencies": {
		"@babel/core": "^7.16.5",
		"@babel/plugin-transform-runtime": "^7.16.4",
		"@babel/preset-env": "^7.16.5",
		"@babel/preset-typescript": "^7.16.5",
		"@changesets/cli": "^2.26.0",
		"@emotion/babel-preset-css-prop": "^11.2.0",
		"@emotion/react": "^11.7.0",
		"@faker-js/faker": "^7.6.0",
		"@manypkg/cli": "^0.20.0",
		"@preconstruct/cli": "^2.5.0",
		"@preconstruct/next": "^4.0.0",
		"@storybook/addon-a11y": "^7.0.3",
		"@storybook/addon-actions": "^7.0.3",
		"@storybook/addon-essentials": "^7.0.3",
		"@storybook/addon-interactions": "^7.0.3",
		"@storybook/addon-links": "^7.0.3",
		"@storybook/builder-vite": "^7.0.3",
		"@storybook/react": "^7.0.3",
		"@storybook/react-vite": "^7.0.3",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/react": "^13.3.0",
		"@testing-library/user-event": "^14.4.3",
		"@types/jest": "^27.0.3",
		"@types/node": "18.14.0",
		"@types/react": "18.0.12",
		"@typescript-eslint/eslint-plugin": "^5.53.0",
		"@typescript-eslint/parser": "^5.53.0",
		"babel-jest": "^27.4.5",
		"babel-loader": "^9.1.2",
		"eslint": "^8.39.0",
		"eslint-config-next": "^13.3.1",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-markdown": "^3.0.0",
		"facepaint": "^1.2.1",
		"html-validate": "^7.4.1",
		"jest": "^27.4.5",
		"plop": "^3.1.1",
		"prettier": "^2.8.4",
		"prettier-plugin-packagejson": "^2.4.3",
		"react": "18.1.0",
		"react-dom": "18.1.0",
		"rimraf": "^3.0.2",
		"storybook": "^7.0.3",
		"storybook-addon-next-router": "^4.0.2",
		"swr": "^1.3.0",
		"typescript": "^4.7.4",
		"vite": "^4.2.1"
	},
	"engines": {
		"node": "18"
	},
	"manypkg": {
		"defaultBranch": "main",
		"ignoredRules": [
			"ROOT_HAS_DEV_DEPENDENCIES"
		]
	},
	"preconstruct": {
		"packages": [
			"packages/*"
		]
	}
}
